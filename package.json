    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "jquery": "^3.3.1",
    "marked": "^0.5.1",
    "mdi": "^2.2.43",
    "sass": "^1.14.3",
    "serve": "^10.1.1"
  },
