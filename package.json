{
  "name": "manbeardgames-generator",
  "version": "1.0.0",
  "description": "Small simple static site generator using Node.js and EJS Templetes.  Used to build the ManBeardGames website",
  "main": "index.js",
  "scripts": {
    "add-game": "node ./scripts/addgame",
    "build": "node -e \"require('./tasks/build-tasks').build();\"",
    "build:prod": "cross-env NODE_ENV=production node .scripts/build",
    "serve": "serve ./public",
    "test-arg": "node -e \"const args = process.argv;  console.log(args['name']);\"",
    "new-controller": "node -e \"if(process.argv.length < 2) { console.error('no name provided'); } else { require('./tasks/controller-tasks').create_controller(process.argv[process.argv.length - 1]); } \"",
    "process-stylesheets": "node -e \"require('./tasks/stylesheet-tasks').process();\""
  },
  "author": "Christohper Whitley",
  "license": "MIT",
  "dependencies": {
    "@mdi/font": "^3.0.39",
    "bootstrap": "^4.1.3",
    "ejs": "^2.6.1",
    "front-matter": "^3.0.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "jquery": "^3.3.1",
    "marked": "^0.5.1",
    "mdi": "^2.2.43",
    "readline-sync": "^1.4.9",
    "sass": "^1.14.3",
    "serve": "^10.1.1"
  },
  "devDependencies": {
    "popper.js": "^1.14.6",
    "prismjs": "^1.15.0",
    "typescript": "^3.3.3333"
  }
}
